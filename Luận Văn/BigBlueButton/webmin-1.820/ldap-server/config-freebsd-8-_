config_file=/usr/local/etc/openldap/slapd.conf
schema_dir=/usr/local/etc/openldap/schema
slapd=/usr/local/libexec/slapd
ldap_user=ldap
browse_max=100
attr_count=6
start_cmd=/usr/local/etc/rc.d/slapd forcestart
stop_cmd=/usr/local/etc/rc.d/slapd forcestop
apply_cmd=/usr/local/etc/rc.d/slapd onerestart
init_name=slapd
